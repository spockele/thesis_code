; AEROLOAD DATA INPUT
; AEROLOAD DATA INPUT
; AEROLOAD DATA INPUT
;
begin aero ;
  nblades  3;
  hub_vec shaft -3 ;          vector from hub (normal to rotor plane) directed towards tower top
  link 1 mbdy_c2_def blade1;
  link 2 mbdy_c2_def blade2;
  link 3 mbdy_c2_def blade3;
  ae_filename        ./data/hawc_ae.001 ;
  pc_filename        ./data/hawc_pc.388 ;
  induction_method   1 ;     0=none, 1=normal
  aerocalc_method    1 ;     0=ingen aerodynamic, 1=med aerodynamic
  aerosections       50 ;
  ae_sets            1 1 1;
  tiploss_method     1 ;     0=none, 1=normal
  dynstall_method    2 ;     0=none, 1=stig øye method,2=mhh method
;
;
; NOISE INPUT
; NOISE INPUT
; NOISE INPUT
;
  begin aero_noise ;
    noise_mode  4 ; 1=Interactive, 2=Store data(for later post-processing)
;                 ; 3=Post-process stored data, 4=Post-process averaged data(Much faster) (Dflt:0)
    output_filename   aeroload  ;
    noise_start_end_time  60.0 63.58 ; 1rot T=3.57s  2rot=57.14s  (Dflt: Noise calculation at all time)
    noise_deltat    0.05 ; [s]  (Dflt: Noise calculation at each HAWC2 time-step)
    noise_azimuth_sectors  32 ; (Dflt: 16)
    atmospheric_pressure  101325.0 ; [Pa]          (Dflt:101325=1bar or atm..?)
    temperature           15.0     ; [^o Celsius]  (Dflt: 20)
   octave_bandwidth       3 ; 0=No noise calculation, 1=1Hz, 3-12-24=octave band widths  (Dflt:3)
   turbulent_inflow_noise 1 ; 0=none, 1= Amiet+VK, 2=Amiet+Mann
   turbulent_inflow_thickness_correction  1 ; 0=none, 1=Moriarty's thickness model
   inflow_turbulence_intensity  08.0 ; [%] (Dflt: 8.0)
