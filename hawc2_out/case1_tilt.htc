; Helge Aagaard Madsen 1-12-2007
; 2MW NM80 Turbine for simulationes to EWEC2008 wake paper
;
begin Simulation;    
  time_stop    70 ;
  solvertype   1 ;    (newmark)
;  animation    ./logfiles_iec/DLC11_ntm_8_s6_pturb_m12_D3.anim;
 logfile       ./log/case1_yaw0deg.log;
;  on_no_convergence  continue;
;
  begin newmark;
    beta      0.27;
    gamma     0.51;  
    deltat    0.05;
    bdynamic  1.0 ;
  end newmark;
end simulation;
;
begin new_htc_structure;
  beam_output_file_name   ./logfiles_iec/beam_master_iec.txt ;
  body_output_file_name   ./logfiles_iec/body_DLC11_ntm_8_s6_pturb_m12_D3.txt ;
;  body_eigenanalysis_file_name ./logfiles_iec/eigen_DLC11_ntm_8_s6_pturb_m12_D3.txt;
;  structure_eigenanalysis_file_name ./logfiles_iec/eigenall_DLC11_ntm_8_s6_pturb_m12_D3.txt;
;
  begin main_body;          tower
    name        tower ;      
    type        timoschenko ;
    nbodies     1 ;
    node_distribution     c2_def ;
    damping   0.01  0.01  0.01   0.001   0.001  0.001 ;
    begin timoschenko_input;
      filename ./data/hawc2_st-radius4004.nm80 ;
      set 4 1;  set subset
    end timoschenko_input;
    begin c2_def;							
	nsec	10	;				
 	sec  1   0   0   0.000 0.00; Ground BC element start
	sec  2  0   0  -0.050 0.00; Ground BC element end
	sec  3  0   0  -3.000 0.00; Foundation top
	sec  4  0   0  -3.875 0.00; Lower flange
	sec  5  0   0 -13.020 0.00;
	sec  6  0   0 -25.000 0.00; Mid flange
	sec  7  0   0 -37.040 0.00;
	sec  8  0   0 -49.000 0.00;
	sec  9  0   0 -57.878 0.00; Nacelle element start
	sec 10  0   0 -59.890 0.00; Tower top						
    end c2_def;	
 end main_body;
;
  begin main_body;
    name        shaft ;       
    type        timoschenko ;
    nbodies     1 ;
    node_distribution     c2_def ;
    damping   0.09  0.09  0.09   0.003   0.003  0.003 ;
    begin timoschenko_input;
      filename ./data/hawc2_st-radius4004.nm80  ;
      set 3 1 ;              
    end timoschenko_input;
    begin c2_def;							
	nsec	5	;				
  	sec 1    0.0	0.0    0.000   0.0 ; Tower top
  	sec 2   0.0	0.0    0.100   0.0 ; Generator end
  	sec 3   0.0	0.0    1.840   0.0 ; Main bearing
  	sec 4   0.0	0.0    2.582   0.0 ; Hub start
  	sec 5   0.0	0.0    4.030   0.0 ; Rotor center
    end c2_def;							
  end main_body;
;
  begin main_body;
    name        hub1 ;       
    type        timoschenko ;
    nbodies     1 ;
    node_distribution     c2_def ;
    damping   0.01  0.01  0.01   0.001   0.001  0.001 ;
    begin timoschenko_input;
      filename ./data/hawc2_st-radius4004.nm80  ;
      set 2 1 ;              
    end timoschenko_input;
    begin c2_def;							
	nsec	4	;				
  	sec 1    0.0	0.0    0.000   0.0 ; Rotor center
  	sec 2   0.0	0.0    0.000500   0.0 ; 
  	sec 3   0.0	0.0    0.001   0.0 ; 
  	sec 4   0.0	0.0    0.00124   0.0 ; Hub start
    end c2_def;							
  end main_body;
;
  begin main_body;
    name           hub2 ;
    copy_main_body hub1;
  end main_body;
;
  begin main_body;
    name           hub3 ;
    copy_main_body hub1;
  end main_body;
;
  begin main_body;
    name        blade1 ;        
    type        timoschenko ;
    nbodies     22;
    node_distribution     c2_def ;
    damping   0.084 0.126 0.028 0.00023 0.0002 0.0002 ;
    begin timoschenko_input;
      filename ./data/hawc2_st-radius4004.nm80 ;
      set 1 1 ;                set subset 
    end timoschenko_input;
