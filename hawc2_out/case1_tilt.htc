; WIND DATA INPUT
; WIND DATA INPUT
; WIND DATA INPUT
;
begin wind ;
  density                 1.231 ;
  wsp                     10.0  ; -> Rot.Speed=1.76rad/s -> T=3.57s
;frba  wsp                     6.1  ; 6.1 m/s 0.deg pitch  1.2881rad/s
  horizontal_input        1     ;
  windfield_rotations     0  0.0  0.0 ;
  center_pos0             0.0 0.0 -59.89 ;  hub_height
;  shear_format            3 0.2;
 shear_format            3 0.025;
  turb_format             0 ; 0: no turbulence, 1:mann, 2:flex
  tower_shadow_method     1     ; 
;   tower_shadow_method     0     ;  
  tint                    0.0 ; 0.1 ; no turbulence?
;  scale_time_start        100 ;
;  wind_ramp_factor 0.0 100.0 0.8 1.0;
;  wind_ramp_abs  50 650 0 20;
;  iec_gust           ;
;
  begin mann;
    filename_u    .\turb\nm80_iec_1006u.bin ;      
    filename_v    .\turb\nm80_iec_1006v.bin ;        
    filename_w    .\turb\nm80_iec_1006w.bin ;      
    box_dim_u   16384 1.031494 ;
    box_dim_v     32 2.8125 ;
    box_dim_w     32 2.8125 ;          
    std_scaling   1.0 0.8 0.5 ;
  end mann;
;
  begin tower_shadow_potential;
    tower_offset 0.0 ;
    nsec  2;
    radius      0.0 2.1 ;
    radius    -60.0 1.25 ;
  end tower_shadow_potential;
;
end wind;
